        "PowerControls": [
            "SetPowerProfile"
        ],
        "WifiConfig": true,
        "CertStore" : true,
        "EnterpriseWifiConfig" : true,
        "NetworkConfig" : true,
        "AllowedTcpServerPorts": [
            8082
