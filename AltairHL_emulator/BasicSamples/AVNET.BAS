100 READING# = 0
200 WHILE 1=1
300 READING# = READING# + 1
400 PRINT "-----------------------------"
500 PRINT "Reading: "; READING#
600 PORT = 63 : PDATA = 0 : GOSUB 1100 : PRINT "Temperature: "; RSTRING$
700 PORT = 63 : PDATA = 1 : GOSUB 1100 : PRINT "Pressure: "; RSTRING$
800 PORT = 64 : PDATA = 0 : GOSUB 1100 : PRINT "Light: "; RSTRING$
900 OUT 30,1 : WAIT 30, 1, 1
1000 WEND
1100 REM SUBROUTINE READS STRING DATA FROM PORT UNTIL NULL CHARACTER
1200 OUT PORT, PDATA
1300 RSTRING$ = ""
1400 C=INP(200)
1500 WHILE C <> 0
1600 RSTRING$ = RSTRING$ + CHR$(C)
1700 C=INP(200)
1800 WEND
1900 RETURN
